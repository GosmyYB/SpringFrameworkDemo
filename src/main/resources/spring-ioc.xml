<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--声明一个id 为 oneInterface. 此后, 通过Spring容器获取该类的一个对象 -->
    <bean id="oneInterface" class="com.wyb.demo.ioc.interfaces.OneInterfaceImpl"></bean>

    <bean id="anotherOneInterface" class="com.wyb.demo.ioc.interfaces.AnotherOneInterfaceImpl"></bean>
</beans>